}

.panel#historyContent tr:first-of-type {
  font-weight: bold;
}

.panel#historyContent tr:nth-of-type(odd) {
  background: #a7383e;
}

.panel#historyContent td, #summary td {
  padding: 0 1.5rem 0 0;
  vertical-align: middle;
  height: 50px;
}

.panel#historyContent td:first-child, #summary td:first-child {
  padding-left: 1rem;
}
