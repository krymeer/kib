* {
  box-sizing: border-box;
  font-family: "Lato", Verdana, Tahoma, sans-serif;
}

html, body {
  margin: 0;
  padding: 0;
  background: #cecece;
  text-align: center;
}

#main {
  margin: 1rem auto;
}

.warningMsg, .errorMsg, .successMsg {
  display: inline-block;
  margin: 2rem 0 0 0;
  padding: 1rem;
  border-radius: 0 16px;
}

.specialButton {
  border: 0;
